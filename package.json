{
    "name": "mongoose-os-ide",
    "displayName": "Mongoose OS IDE",
    "description": "Mongoose OS IDE for Visual Studio Code",
    "version": "0.0.1",
    "publisher": "ih8c0ff33",
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:mongoose-os-ide.testCommand"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "mongoose-os-ide.testCommand",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
